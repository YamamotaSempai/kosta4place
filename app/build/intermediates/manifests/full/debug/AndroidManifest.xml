<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kost4place.aa.kz.kosta4place"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="27" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="kost4place.aa.kz.kosta4place.ui.HomePage"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="kost4place.aa.kz.kosta4place.ui.Categories"
            android:screenOrientation="portrait" />
        <activity
            android:name="kost4place.aa.kz.kosta4place.ui.AboutPlace"
            android:screenOrientation="portrait" />

        <provider
            android:name="com.squareup.picasso.PicassoProvider"
            android:authorities="kost4place.aa.kz.kosta4place.com.squareup.picasso"
            android:exported="false" />
    </application>

</manifest>